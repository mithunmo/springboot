package com.f9g4.common.util;

public enum ContestEnum {
	//project type
	TYPE_DEFAULT(0,"Default"),
	TYPE_COMMISSIONED(1,"Commissioned"), 
	TYPE_INNER_CIRCLE(2,"Inner Circle"),
	TYPE_MEMBERS_ONLY(3,"Members Only"),
	TYPE_PUBLIC_CONTEST(4,"Public Contest"),
	//project status
	STATUS_SUSPENDED(0,"Suspended"), 
	STATUS_PUBLISHED(1,"Published"), 
	STATUS_CANCEL(2,"Cancelled"), 
	STATUS_COMPLETED(3,"Completed"),
	STATUS_RUNNING(4,"Running"), 
	STATUS_PENDING(5,"Pending"),
	STATUS_DEFAULT(6,"Default");
	
	private final int id;
	private final String code;
	
	ContestEnum(int id, String code){
		this.id = id;
		this.code = code;
	}

	public int getId() {
		return id;
	}
	public String getCode() {
		return code;
	}
	
}
