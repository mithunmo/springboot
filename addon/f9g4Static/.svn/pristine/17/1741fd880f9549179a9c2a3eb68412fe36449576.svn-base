/*Basic components==========================================================*/
.button
{
	display:inline-block;
	background-color:#00AEEF;
	color:#FFF;
	padding:5px 10px;
	cursor:pointer;
	text-align:center;
	margin-right:20px;
	min-width:70px;
	/*font-weight:bold;*/
	/*border:3px solid #000;*/
	font-size:1.063em;
	/*text-transform: uppercase;*/
	text-decoration: none !important;
}
.button-holder { width:990px; margin:0px auto; clear:both; overflow: hidden;}

a:hover.button { color:#FFF;}
.button.center
{
	float:none !important;
	margin-left:auto !important;
	margin-right:auto !important;
}

.button.disabled,.button[button-disabled='true'],.button[disabled='disabled']
{
	cursor:default;
	background-color: #0B3458;
}

.button.gray { background-color:#58595B;}
.button.critical { background-color: #0B3458;}

.orange
{
	background-color:#C30;
}

.upArrow
{
	transform:rotate(180deg);
	-ms-transform:rotate(180deg); /* IE 9 */
	-moz-transform:rotate(180deg); /* Firefox */
	-webkit-transform:rotate(180deg); /* Safari and Chrome */
	-o-transform:rotate(180deg); /* Opera */
}
.empty { position:absolute; top:50%; left:50%; width:300px; height:150px; margin-top:-75px; margin-left:-160px; background-color:#BCBEC0; text-align: center; /* border-radius: 10px; */ opacity:1.0; z-index:10;}
.empty span { margin-top:50px; display:inline-block; color:#C30;}
.empty.opa { background-color:#000; /* IE 8 */ -ms-filter: "progid:DXImageTransform.Microsoft.Alpha(Opacity=50)"; /* IE 5-7 */ filter: alpha(opacity=50); /* Netscape */ -moz-opacity: 0.5; /* Safari 1.x */ -khtml-opacity: 0.5; /* Good browsers */ opacity: 0.5;}
.empty.opa span { color:#FFF;}
.empty.w { width:450px; margin-left:-225px;}
.empty.w span { font-size: 1.2em;}
a.act { color:#FFF;}
.empty.w .close { position:absolute; top:5px; right:5px; color:#FFF;}
.loadingSpinner { position:absolute; top:50%; left:50%; width:300px; height:150px; margin-top:-75px; margin-left:-160px; background-color:#BCBEC0; text-align: center; /* border-radius: 10px; */ opacity:1.0; z-index:3;}
.loadingSpinner span { margin-top:50px; display:inline-block; color:#C30;}
.loadingSpinner.disable { display:none;}
#emptySearch span { margin-top:45px;}
#empty_search_ok { margin-left:100px; margin-top:20px;}
.loading { position:absolute; top:50%; left:50%; width:300px; height:150px; margin-top:-75px; margin-left:-160px; background:#BCBEC0; text-align: center; /* border-radius: 10px; */}
.loading span { margin-top:70px; display:inline-block; color:#C30;}
.context-menu { position:fixed; z-index:99999; width:150px; height:50px; background-color: #FFF; display:none; border:thin solid #CCC;}
.context-menu.shadow { -moz-box-shadow: 5px 5px 5px #888; -webkit-box-shadow: 5px 5px 5px #888; box-shadow: 5px 5px 5px #888; -ms-filter: "progid:DXImageTransform.Microsoft.Shadow(Strength=5, Direction=135, Color='#888888')"; filter: progid:DXImageTransform.Microsoft.Shadow(Strength=5, Direction=135, Color='#888888');}
.download-hint { position:fixed; bottom:0px; width:100%; height:30px; line-height:30px; text-align:center; background-color:#FCFFCE; z-index:99998;}
.nav { position:absolute; z-index:9999; width:40px; height:53px; background-image: url('/images/g_arrow_up.png'); cursor:pointer;}
label.highlight { border:2px solid #C30;}
.comm-protocol-holder { text-align: justify; display:none;}
/*End of basic components============================================================*/

