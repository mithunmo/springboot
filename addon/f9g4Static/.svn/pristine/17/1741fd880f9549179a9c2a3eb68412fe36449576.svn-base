/*Global Setting ******************************************************************/
body
{
	color:#000;
	font-family: "HelveticaNeue-Light", "Helvetica Neue Light", "Helvetica Neue", Helvetica, Arial, "Lucida Grande", sans-serif;
	font-size:0.875em;
	letter-spacing: 0px;
	word-spacing:0px;
	width:100%;
	margin:0px;
	z-index:1;
	background: #FFF; /* Old browsers */
}

img { border-style: none;}/* to remove the blue border around the image link.*/

a { text-decoration: underline; cursor:pointer; color:#000; margin: 0; padding: 0; border: 0;}
a:hover { color:blue;}

ul { padding-left:10px;}
input[type=text] { border:1px solid #000; font-size: 0.938em; border-radius:0px;}
select { border:1px solid #000; font-size: 0.938em; border-radius:0px;}
textarea { border:1px solid #000; font-size:0.938em; border-radius:0px;}
pre { font-family: inherit; font-size: inherit; letter-spacing:inherit; word-break:break-word;}

/*Table */
.tableHeader-holder { width:100%; background-color: #00AEEF;}

.font-header { font-size:1.063em;}
.font-header.cap-bold { text-transform: uppercase; font-weight: bold;}
.font-regular {font-size:1em;}
.font-dashboard {font-size:1em; font-weight:bold;}

