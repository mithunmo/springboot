@charset "utf-8";
/* CSS Document */

img { border-style: none;} /* to remove the blue border around the image link.*/

/*Viewer========================================*/
#dialog
{
	z-index:-1;
}

.popupContent
{
	display:none;
	height:100%;
	overflow:hidden;
}

.dialogLeftPanel
{
	clear:left;
	display:inline-block;
	overflow:hidden;
	width:59%;
}

.dialogPic
{
	top:0px;
	display:block;
	width:99%;
	height:300px;
	border:thin solid #000;
	overflow:hidden;
}

.dialogDetailLeft
{
	margin-top:10px;
	display:block;
	width:99%;
	height:200px;
	border:thin solid #000;
	overflow:hidden;
}

.dialogDetailRight
{
	top:0px;
	display:inline-block;
	float:right;
	width:38%;
	padding:1%;
	border:thin solid #666;
	border-radius:25px;
	overflow:hidden;
	behavior: url(/PIE.htc);
}

.prevItem
{
	display:inline-block;
	width:10%;
	height:100%;
	z-index:2;
}

.nextItem
{
	float:right;
	display:inline-block;
	width:10%;
	height:100%;
	z-index:2;
}
/*End of viewer=======================================================================*/


/*=====================POPUP===========================================================*/
.overlay /*Gray cover*/
{
	position:fixed;
	background-color:#666;
	opacity:0.7;
	z-index:10;
	display:none;
	width:100%;
	height:100%;
	top:0px;
	left:0px;
}

#session_overlay {z-index:15;}
#error_overlay {z-index:12;}

.popup .notification
{
	clear:both;
	display:none;
	/* width:200px; */
	color:#F00;
}
/*basic component--------------------------------------*/
.popup .orangeText{ color:#C30;}
.popup .fortyPercent {width:39%;}
.popup .sixtyPercent {width:59%;}
.popup input[type=text] {width:90%;}
.popup textarea {width:95%; resize:none;}
.popup .row { width:100%; clear:both; padding-bottom:10px;}
.popup .column { display:inline-block;}




/*button layout----------------------------------------*/
.popup .buttonPanel
{
	width:90%;
	position:absolute;
	clear:left;
	margin-top:5px;
	margin-bottom:5px;
	margin-left:auto;
	margin-right: auto;
	text-align:center;
	/*top:190px;*/
}
/* .popup .buttonPanel .button { float:right;} */
.popup .button
{
	display:inline-block;
	margin-top:8px;
	margin-left:5px;
	margin-right:5px;
	background-color:#0457A0;
	color:#FFF;
	border-radius:10px;
	font-size:14px;
	padding:5px 20px;
	cursor:pointer;
	float:none;
	text-align:center;
	min-width:40px;
	behavior: url(/PIE.htc);
}

.popup .orangeButton{ background-color:#C30;}

/*for from dropdown------------*/
.popup .dropdown
{
	width:254px;
	margin-top:5px;
	margin-bottom:5px;
}

.popupContainer
{
	display:none;
	position:absolute;
	z-index:11;
}

.popup
{
	font-family:Arial, Helvetica, sans-serif;
	/*overflow:hidden;*/
	width:443px;
	min-height:200px;
	/*max-height:439px;*/
	background-color:#FFF;
	border-radius:10px;
	/*shadow*/
	box-shadow:10px 10px 10px #333333;
	behavior: url(/PIE.htc);
}

.popup .header
{
	display:block;
	width:90%;
	height:33px;
	font-size:18px;
	background-color:#bdcbd4;
	/* background-color:#FF0000; */
	padding-top:15px;
	padding-left:5%;
	padding-right:5%;
	color:#330066;
	overflow:hidden;
	border-radius:10px 10px 0px 0px;
	font-family: 'Arial Narrow', sans-serif;
	/* z-index:-1; */
	behavior: url(/PIE.htc);
}

.popup .header .close
{
	display:inline-block;
	float:right;
	/*padding-right:10px;*/
	font-size:14px;
	cursor:pointer;
}

.popup .header .error_close
{
	display:inline-block;
	float:right;
	/*padding-right:10px;*/
	font-size:14px;
	cursor:pointer;
}

.popup .content
{
	display:block;
	/*overflow:hidden;*/
	padding:10px;
}

/*Terms panel=======================================================*/
#terms{ height:452px;}
.popup .terms
{
	font-size:14px;
	margin:20px;
	/*height:370px;*/
}
#agree{ color:#660;}
#decline{ color:#F00;}
#terms_ok{ width:40px; margin-left:180px; margin-bottom:10px;}
#terms_ok_login{ width:40px; margin-left:180px; margin-bottom:10px;}
.popup .confirmation
{
	background-color:#bdcbd4;
	text-align:center;
	width:385px;
	height:30px;
}
/*scrollbar for terms panel---------------------------*/
#termScrollbar 
{
	width: 403px; 
	height:300px;
	clear: both; 
	margin: 0px;
}
#termScrollbar .viewport 
{ 
	width: 385px; 
	height: 300px; 
	overflow: hidden; 
	position: relative;
}
#termScrollbar .overview 
{ 
	list-style: none; 
	position: absolute; 
	left: 0; 
	top: 0;
	padding-left:10px;
	word-wrap:break-word;
}
#termScrollbar .thumb .end,
#termScrollbar .thumb 
{
	background-color: #919295;
	opacity:0.5;
}
#termScrollbar .scrollbar
{ 
	position: relative; 
	float: right; 
	width: 15px;
}
#termScrollbar .track 
{ 
	background-color: #FFF; 
	height: 100%; 
	width:10px; 
	position: relative; 
	padding: 0 1px;
}
#termScrollbar .thumb 
{ 
	height: 20px; 
	width: 8px; 
	cursor: pointer; 
	overflow: hidden; 
	position: absolute; 
	top: 0;
	border-radius:10px;
	behavior: url(/PIE.htc);
}
#termScrollbar .thumb .end 
{ 
	overflow: hidden; 
	height: 5px; 
	width: 13px;
	z-index:-1;
	border-radius:10px;
	behavior: url(/PIE.htc);
}
#termScrollbar .disable
{ 
	display: none; 
}
/*===Ends of terms panel================================================*/


