.iih-show-hide{-webkit-transition:max-height ease 1s;transition:max-height ease 1s;max-height:2000px;overflow:hidden}.iih-show-hide.ng-hide{max-height:0}.sync{-webkit-transition:all ease .3s;transition:all ease .3s;opacity:1}.sync.ng-hide{height:0;opacity:0}.msg-box-container{-webkit-transition:all linear .5s;transition:all linear .5s;max-height:500px}.msg-box-container.ng-hide{max-height:0;opacity:0}.animate-show{line-height:20px;opacity:1;padding:10px;border:1px solid #000;background:#fff;-webkit-transition:all linear .5s;transition:all linear .5s}.animate-show.ng-hide{line-height:0;opacity:0;padding:0 10px}.check-element{padding:10px;border:1px solid #000;background:#fff}