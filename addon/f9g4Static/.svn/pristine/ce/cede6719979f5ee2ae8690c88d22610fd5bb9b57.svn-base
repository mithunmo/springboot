/* CSS Document */
body
{
	margin:0px;
}

img { border-style: none;} /* to remove the blue border around the image link.*/

.body
{
	margin-left:auto;
	margin-right:auto;
	font-family:Arial, Helvetica, sans-serif;
	width:980px;
	/*height:750px;*/
	/*border-left:thin solid #999;*/
	/*border-right:thin solid #999;*/
	color:#330066;
}

.body div
{
	float:left;
	display:block;
}
.banner
{
	position:absolute;
	top:0px;
	width:100%;
	height:33px;
	background-color:#BDCBD4;
	z-index:-1;
}
.up
{
	padding-top:5px;
	top:0px;
	width:960px;
	height:31px;
	font-style:italic;
	font-size:16px;
	color:#330066;
	text-align:right;
	padding-right:20px;
	/*border:thin solid #000;*/
}

.left
{
	clear:left;
	display:inline-block;
	top:31px;
	width:11px;
	/*height:800px;*/
	/*border:thin solid #000;*/
}

.middle
{
	display:inline-block;
	top:31px;
	width:960px;
	height:670px;
}

/*panel for about_us, work_with_us,contact_us,and privacy_policy*/
#singlePanel
{
	width:960px;
	height:600px;
	margin-top:20px;
}

#panelheader
{
	width:960px;
	height:28px;
}

#panelContent
{
	width:960px;
	height:550px;
	
}

.right
{
	display:inline-block;
	width:5px;
	/*height:800px;*/
	float:right;
}

.mainbox
{
	width:950px;
	height:450px;
	/* background-image:url(/images/SS_Main-Img.gif); */
	background: url(/images/SS_Main-Img.gif) no-repeat;
	-pie-background: url(/images/SS_Main-Img.gif) no-repeat; /*PIE*/
}

.mainbox .logo
{
	width:211px;
	height:126px;
	margin-top:10px;
	/*border:thin solid #000;
	border-radius:10px;*/
	background-image:url(/images/Small-Logo.gif);
}

.mainbox .loginPanel
{
	margin-top:10px;
	float:right;
	width:220px;
	height:240px;
	overflow:hidden;
	font-size:11px;
	color:#330066;
	padding-right:20px;
	font-weight:bold;
	position: relative;
	z-index:0;
	/*border:thin solid #000;*/
}

.loginPanel .userinfo
{
}

.loginPanel .remember
{
	clear:both;
	margin-top:10px;
}

.loginPanel .forgetCheck
{
	claer:both;
	margin-top:5px;
	margin-left:21px;
}

.loginPanel .submit
{
	clear:both;
	margin-top:8px;
	width:auto;
	background-color:#0457A0;
	color:#FFF;
	float:right;
	font-size:14px;
	font-weight:normal;
	padding:5px 20px;
	cursor:pointer;
}

#submit { z-index:10; border-radius:10px; box-shadow:5px 5px 5px #333;  behavior: url(/PIE.htc);}

.loginPanel a
{
	font-weight:bold;
}

.curve
{
	border-radius:8px;
	width:200px;
	height:20px;
	color:#CCC;
	behavior: url(/PIE.htc);
}

.textField
{
	border-radius:8px;
	width:200px;
	height:20px;
	/*behavior: url(/PIE.htc);*/
}

.space
{
	width:950px;
	height:30px;
	/*border:thin solid #000;*/
}

/*bottom panl---------------------------------*/
.bottomPanel
{
	width:960px;
	height:183px;	
}

.singlePanel
{
	display:inline-block;
	width:310px;
	height:183px;
	border:1px solid #330066;
	border-radius:20px;
	overflow:hidden;
	behavior: url(/PIE.htc);
}

.middleSinglePanel
{
	display:inline-block;
	width:310px;
	height:183px;
	margin-left:10px;
	margin-right:10px;
	border:1px solid #330066;
	border-radius:20px;
	overflow:hidden;
	behavior: url(/PIE.htc);
}

.panelheader
{
	width:300px;
	height:28px;
	font-size:21px;
	text-shadow: 3px 3px 3px #333333;
	color:#FFF;
	background-color:#bdcbd4;
	overflow:hidden;
	padding-left:10px;
	padding-top:3px;
	border-radius: 20px 20px 0px 0px;
	behavior: url(/PIE.htc);
}

.panelContent
{
	width:300px;
	height:150px;
	font-size:12px;
}

.socialButtons
{
	display:inline-block;
	float:right;
	margin-top:3px;
	margin-right:15px;
}

.socialButtons img
{
	margin-right:0px;
}

.down
{
	margin-top:15px;
	width:960px;
	height:60px;
	text-align:center;
	/*border:thin solid #000;*/
}

.down .navi
{
	width:inherit;
	border-top: thin solid #bdcbd4;
	border-bottom:thin solid #bdcbd4;
	margin-top:10px;
	padding-top:5px;
	padding-bottom:5px;
	text-align:center;
}

.down .navi a
{
	margin-left:5px;
	font-weight:bold;
	text-decoration:none;
	color:#006;
	font-size:12px;
}

.down .copyright
{
	width:950px;
	clear:both;
	color:#333;
	font-size:10px;
}

#debug
{
	position:fixed;
	display:none;
	top:0;
	left:0;
	z-index:20;
	width:200px;
	height:200px;
	background-color:#FFF;
	border:thin solid #000;
	overflow:scroll;
}

/*CSS for placeholder---------------------*/
.placeholder { color: #aaa; }

/*==============Footer Page============================*/
#enlargeSinglePanel { width:960px; height:640px; margin-top:20px;}
#enlargePanelHeader { width:950px; height:40px; text-align: center; font-size: 30px;}
#enlargePanelContent { width:960px; }
/*scrollbar for footer page*/
#scrollbarFooter
{
	width: 960px; 
	clear: both; 
	margin: 0px;
}

.entity
{
}

#scrollbarFooter .viewport 
{ 
	width: 940px; 
	height: 590px; 
	overflow: hidden; 
	position: relative;
}
#scrollbarFooter .overview 
{ 
	
}
#scrollbarFooter .thumb .end,
#scrollbarFooter .thumb 
{
	background-color: #919295;
	-ms-filter: "progid:DXImageTransform.Microsoft.Alpha(Opacity=50)";
	/*opacity:0.5;*/
}
#scrollbarFooter .scrollbar 
{ 
	display:none;
	position: relative; 
	float: right; 
	width: 15px;
	/*z-index:-1;*/
}
#scrollbarFooter .track 
{ 
	background-color: #FFF; 
	height: 100%; 
	width:0px; 
	position: relative; 
	padding: 0 1px;
}
#scrollbarFooter .thumb 
{ 
	height: 20px; 
	width: 8px; 
	cursor: pointer; 
	overflow: hidden; 
	position: absolute; 
	top: 0;
	/* border-radius:10px;
	behavior: url(/PIE.htc); */
}
#scrollbarFooter .thumb .end 
{ 
	overflow: hidden; 
	height: 5px; 
	width: 13px;
	/* border-radius:10px;
	behavior: url(/PIE.htc); */
}
#scrollbarFooter .disable
{ 
	display: none; 
}


