/* login panel*/
#loginPanel { position:absolute; width:100%; height:100%; z-index:6; display:none; font-size: 19px; left:0px; top:0px;}
#loginPanel .content-holder { position:relative; top:20%; background-color: #FFF; border: 10px solid #00AEEF; width:450px; height:350px; z-index:8; margin:0px auto; padding:60px 60px; float:none;}
