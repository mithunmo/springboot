.b-shadow { -moz-box-shadow: 5px 5px 5px #CCC; -webkit-box-shadow: 5px 5px 5px #CCC; box-shadow: 5px 5px 5px #CCC; -ms-filter: "progid:DXImageTransform.Microsoft.Shadow(Strength=5, Direction=135, Color='#CCCCCC')"; filter: progid:DXImageTransform.Microsoft.Shadow(Strength=5, Direction=135, Color='#CCCCCC');}
.b-radius { -webkit-border-radius: 3px; -moz-border-radius: 3px; border-radius: 3px;}

