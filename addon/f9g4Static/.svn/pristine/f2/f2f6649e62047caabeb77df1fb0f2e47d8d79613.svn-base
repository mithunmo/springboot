#loginPanel .overlay 
{ 
	position:fixed; 
	background-color:#666; 	 
	z-index:7; 
	display:block; 
	width:100%; 
	height:100%; 
	top:0px; 
	left:0px;
	/* IE 8 */
	-ms-filter: "progid:DXImageTransform.Microsoft.Alpha(Opacity=70)";
	/* IE 5-7 */
	filter: alpha(opacity=70);
	/* Netscape */
	-moz-opacity: 0.7;
	/* Safari 1.x */
	-khtml-opacity: 0.7;
	/* Good browsers */
	opacity: 0.7;
}
