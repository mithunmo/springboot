@charset "utf-8";
/* CSS Document */
@font-face {
    font-family: 'BebasNeueRegular';
    src: url('/font/BebasNeue-webfont.eot');
    src: url('/font/BebasNeue-webfont.eot?#iefix') format('embedded-opentype'),
         url('/font/BebasNeue-webfont.woff') format('woff'),
         url('/font/BebasNeue-webfont.ttf') format('truetype'),
         url('/font/BebasNeue-webfont.svg#BebasNeueRegular') format('svg');
    font-weight: normal;
    font-style: normal;
}

@font-face {
  font-family: "Raleway";
  font-style: normal;
  font-weight: 400;
  src: local("Raleway"), url("http://themes.googleusercontent.com/static/fonts/raleway/v7/IczWvq5y_Cwwv_rBjOtT0w.woff") format("woff");
}

body
{
	margin:0px;
	color:#000;
	font-family: Arial Narrow, Calibri, 'Arial Bold', Arial, sans-serif;
	font-size:0.938em;
	letter-spacing: 0px;
	word-spacing:0px;
	background: #e6e7e8; /* Old browsers */
	/* IE9 SVG, needs conditional override of 'filter' to 'none' */
	background: url(data:image/svg+xml;base64,);
	background: -moz-linear-gradient(top,  #e6e7e8 0%, #ffffff 30%, #ffffff 70%, #e6e7e8 100%); /* FF3.6+ */
	background: -webkit-gradient(linear, left top, left bottom, color-stop(0%,#e6e7e8), color-stop(30%,#ffffff), color-stop(70%,#ffffff), color-stop(100%,#e6e7e8)); /* Chrome,Safari4+ */
	background: -webkit-linear-gradient(top,  #e6e7e8 0%,#ffffff 30%,#ffffff 70%,#e6e7e8 100%); /* Chrome10+,Safari5.1+ */
	background: -o-linear-gradient(top,  #e6e7e8 0%,#ffffff 30%,#ffffff 70%,#e6e7e8 100%); /* Opera 11.10+ */
	background: -ms-linear-gradient(top,  #e6e7e8 0%,#ffffff 30%,#ffffff 70%,#e6e7e8 100%); /* IE10+ */
	background: linear-gradient(to bottom,  #e6e7e8 0%,#ffffff 30%,#ffffff 70%,#e6e7e8 100%); /* W3C */
}

img { border-style: none;} /* to remove the blue border around the image link.*/

textarea { font-family:Arial, Helvetica, sans-serif; width:90%;}

.require_field { color:#FF0000; font-size:14px;}

.button
{
	display:inline-block;
	background-color:#00AEEF;
	color:#FFF;
	padding:2px 10px;
	cursor:pointer;
	text-align:center;
	margin-right:20px;
	min-width:70px;
	font-weight:bold;
	border:3px solid #000;
	font-size:1.063em;
	text-transform: uppercase;
	text-decoration: none;
}

.button.center
{
	float:none;
	margin-left:auto;
	margin-right:auto;
}

