#loginPanel .content-holder .username { margin-bottom: 20px;}
#loginPanel .content-holder .username span { font-weight: bold;}
#loginPanel .content-holder .username input { width:100%; margin:0px; padding:0px; font-size: 19px; border: 2px solid #000;}
#loginPanel .content-holder .password { margin-bottom: 20px;}
#loginPanel .content-holder .password span { font-weight: bold;}
#loginPanel .content-holder .password input { width:100%; margin:0px; padding:0px; font-size: 19px; border: 2px solid #000;}
